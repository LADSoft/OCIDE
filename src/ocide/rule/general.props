				<SETTING NAME="No" VALUE="0"/>
				<SETTING NAME="Yes" VALUE="1"/>
			</ITEM>
			<ITEM NAME="Column Mark" ID="COLUMN_MARK" TYPE="NUMERIC" LOW="0" HIGH="300" DEFAULT="80"/>
