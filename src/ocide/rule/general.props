			</ITEM>
			<ITEM NAME="Code completion threshold(variables)" ID="CODE_COMPLETION_VARIABLE_THRESHOLD" TYPE="NUMERIC" LOW="1" HIGH="10" DEFAULT="3"/>
			<ITEM NAME="Code completion threshold(keywords)" ID="CODE_COMPLETION_KEYWORD_THRESHOLD" TYPE="NUMERIC" LOW="1" HIGH="10" DEFAULT="2"/>
		
			<ITEM NAME="Browse Information" ID="BROWSE_INFORMATION" TYPE="COMBO" DEFAULT="1">
				<SETTING NAME="No" VALUE="0"/>
				<SETTING NAME="Yes" VALUE="1"/>
			</ITEM>
